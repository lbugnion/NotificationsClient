{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "logLevel": {
    "default": "Debug",
    "Host.Results": "Error",
    "Function": "Error",
    "Host.Aggregator": "Information"
  }
}